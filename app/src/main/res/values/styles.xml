<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- 应用的主要色调，actionBar默认使用该颜色，Toolbar导航栏的底色-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/white</item>
        <!--控制各个控件被选中时的颜色-->
        <item name="colorAccent">@color/colorAccent</item>
        <!--页面背景色-->
        <item name="android:windowBackground">@color/root_bg</item>
        <!--api 21 可用 各个控制控件的默认颜色-->
        <item name="android:colorControlNormal">@color/colorControlNormal</item>
        <item name="android:screenOrientation">portrait</item>

        <item name="android:selectableItemBackground">@null</item>
        <item name="android:selectableItemBackgroundBorderless">@null</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>

    <style name="MainActivityTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- 应用的主要色调，actionBar默认使用该颜色，Toolbar导航栏的底色-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/white</item>
        <!--控制各个控件被选中时的颜色-->
        <item name="colorAccent">@color/colorAccent</item>
        <!--页面背景色-->
        <item name="android:windowBackground">@color/root_bg</item>
        <!--api 21 可用 各个控制控件的默认颜色-->
        <item name="android:colorControlNormal">@color/colorControlNormal</item>
        <item name="android:screenOrientation">portrait</item>
    </style>

    <style name="text_12sp">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/text_norm_black</item>
    </style>

    <style name="text_14sp">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_norm_black</item>
    </style>

    <style name="mine_module_more">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#818384</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="mine_module_list">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:nestedScrollingEnabled">false</item>
    </style>

    <style name="mine_module_list_other">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:nestedScrollingEnabled">false</item>
    </style>


    <declare-styleable name="MaxHeightRecyclerView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>


    <style name="hint_Title">
        <item name="android:textColor">@color/text_norm_black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="dialogBase" parent="@android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 变暗 -->
        <item name="android:backgroundDimEnabled">true</item>

        <item name="android:backgroundDimAmount">0.4</item>
    </style>

    <!--Mine-->
    <style name="mine_module_card">
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:layout_marginEnd">@dimen/dp_16</item>
        <item name="android:layout_marginStart">@dimen/dp_16</item>
