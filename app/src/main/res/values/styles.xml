    <!-- 图片选择对话框Style-->
    <style name="popup_dialog_anim" parent="@style/popup_dialog">
        <item name="android:windowAnimationStyle">@style/dialog_animations</item>
    </style>

    <style name="dialog_animations">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>


    <!--特定字体的UI-->
    <style name="formTitleStyle">
        <item name="android:layout_height">56dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_norm_black</item>
    </style>
    <!--特定输入框的UI-->
    <style name="formInputStyle">
        <item name="android:layout_height">56dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">end|center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_norm_black_40_alpha</item>
        <item name="android:hint">请输入</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginEnd">@dimen/dp_16</item>
        <item name="android:minWidth">200dp</item>
    </style>

    <style name="pwdInputStyle">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:inputType">numberPassword</item>
        <item name="android:textColor">@color/regular_black</item>
        <item name="android:textSize">22sp</item>
        <item name="android:background">@drawable/shape_pwd_radius4</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:maxLength">1</item>
    </style>

    <style name="bindPhotoInputPwdStyle">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:inputType">phone</item>
        <item name="android:textColor">@color/regular_black</item>
        <item name="android:textSize">24sp</item>
        <item name="android:background">@drawable/shape_pwd_radius4</item>
        <item name="android:gravity">center</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:maxLength">1</item>
    </style>

    <!--invite-->
    <style name="invite_step_text">
        <item name="android:layout_marginTop">45dp</item>
        <item name="android:layout_width">12dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">8sp</item>
        <item name="background_normal">@color/text_red_color_60_alpha</item>
        <item name="corner_radius">12dp</item>
    </style>

    <style name="wxAuthActivity" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <style name="common_badge" parent="common_badge_no_border">
        <item name="border_color_normal">@color/white</item>
        <item name="border_width_normal">1dp</item>
    </style>

    <style name="common_badge_no_border">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">14dp</item>
        <item name="android:minWidth">14dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">3dp</item>
        <item name="android:paddingEnd">3dp</item>
        <item name="text_color_normal">@color/white</item>
        <item name="android:textSize">8sp</item>
        <item name="background_normal">@color/text_main_red_color</item>
        <item name="corner_radius">7dp</item>
    </style>

    <!-- 圆角图片 -->
    <style name="roundedCornerImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/bg_norm_radius</item>
    </style>

    <style name="roundedCorner4ImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>

    <!-- 圆形图片 -->
    <style name="circleImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <!--省市区街道-->
    <style name="address_level_text" parent="text_black_14">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxWidth">70dp</item>
        <item name="android:maxLines">2</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>